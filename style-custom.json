            "type": "vector",
            "tiles": [
                "https://aips.siniswift.com/gis/spaces/features/BASE/{z}_{x}_{y}.mvt"
            ],
            "minzoom": 0,
            "maxzoom": 24
        },
        "maplibreCountries": {
            "type": "vector",
            "tiles": [
                "https://api.maptiler.com/tiles/land/{z}/{x}/{y}.pbf?key=0fZ6KQdoBr7MjJofYHkh"
            ],
            "minzoom": 0,
            "maxzoom": 24
        },
