                "line-color": "#eecf68",
                "line-opacity": 0.8,
                "line-width": [
                    "interpolate",
                    [
                        "linear"
                    ],
                    [
                        "zoom"
                    ],
                    1,
                    1,
                    2,
                    2
                ]
            },
