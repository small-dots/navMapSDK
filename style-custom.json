            "minzoom": 0,
            "maxzoom": 24
        },
        "AMMMvt": {
            "type": "vector",
            "tiles": [
                "https://aips.siniswift.com/gis/spaces/features/AMM/{z}_{x}_{y}.mvt"
            ],
            "minzoom": 9,
            "maxzoom": 24
        },
